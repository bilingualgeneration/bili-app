    "cypress": "^13.3.0",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.14",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "3.1.0",
    "sass": "^1.69.2",
    "storybook": "^7.4.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.3.9",
