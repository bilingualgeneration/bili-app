{
  "name": "@bili/app",
  "packageManager": "yarn@4.0.1",
  "private": true,
  "version": "1.0.4+1",
  "type": "module",
  "scripts": {
    "build": "run-s build:development",
    "build:development": "run-s build:vite:development",
    "build:emulator": "run-s build:vite:emulator",
    "build:live": "run-s build:vite:live",
    "build:ios:live": "run-s build:vite:live cap:sync cap:open:ios",
    "build:vite:development": "tsc; vite build --mode development",
    "build:vite:emulator": "tsc; vite build --mode emulator",
    "build:vite:live": "tsc; vite build --mode live",
    "serve:vite:development": "vite --mode development",
    "serve:vite:emulator": "vite --mode emulator",
    "serve:vite:live": "vite --mode live",
    "serve": "run-s serve:vite:development",
    "serve:development": "run-s serve:vite:development",
    "serve:live": "run-s serve:vite:live",
    "serve:emulator": "run-s serve:vite:emulator",
    "cap:sync": "cap sync",
    "cap:open:ios": "cap open ios",
    "preview": "vite preview",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "lint": "eslint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "extract-translations": "formatjs extract 'src/**/*.ts*' --ignore '**/*.d.ts' --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format=simple",
    "compile-translations:en": "formatjs compile lang/en.json --ast --out-file compiled-lang/en.json --format=simple",
    "compile-translations:es": "formatjs compile lang/es.json --ast --out-file compiled-lang/es.json --format=simple",
    "compile-translations:all": "yarn compile-translations:en; yarn compile-translations:es",
    "open:ios:live": "run-s build:live cap:sync cap:open:ios"
  },
  "dependencies": {
    "@babel/cli": "^7.23.0",
    "@bili/schema": "workspace:^",
    "@capacitor/app": "5.0.6",
    "@capacitor/browser": "^5.1.0",
    "@capacitor/core": "5.7.0",
    "@capacitor/device": "^5.0.7",
    "@capacitor/haptics": "5.0.6",
    "@capacitor/ios": "^5.7.0",
    "@capacitor/keyboard": "5.0.6",
    "@capacitor/preferences": "^6.0.0",
    "@capacitor/splash-screen": "^6.0.0",
    "@capacitor/status-bar": "5.0.6",
    "@formatjs/intl-pluralrules": "^5.2.7",
    "@formatjs/intl-relativetimeformat": "^11.2.7",
    "@hookform/resolvers": "^3.3.1",
    "@ionic/react": "^7.0.0",
    "@ionic/react-router": "^7.0.0",
    "@ionic/react-test-utils": "^0.4.0",
    "@maskito/core": "^1.9.0",
    "@maskito/react": "^1.9.0",
    "@react-spring/web": "9.5.5",
    "@svgr/core": "^8.1.0",
    "@tanstack/react-table": "^8.17.3",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "babel-plugin-react-intl": "^8.2.25",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "firebase": "^10.4.0",
    "immutability-helper": "^3.1.1",
    "ionicons": "^7.0.0",
    "rdndmb-html5-to-touch": "^8.0.3",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-multi-backend": "^8.0.3",
    "react-dnd-preview": "^8.0.3",
    "react-dnd-touch-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.47.0",
    "react-intl": "^6.5.1",
    "react-intl-universal": "^2.7.5",
    "react-joyride": "^2.7.1",
    "react-lorem-ipsum": "^1.4.10",
    "react-redux": "^8.1.3",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "react-slick": "^0.30.2",
    "react-tinder-card": "^1.6.4",
    "react-use": "^17.4.2",
